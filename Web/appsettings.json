{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BillingSettings": {
    "ReportCost": 500.00,
    "FreePeriodHours": 24
  },
  "CacheSettings": {
    "ExpirationHoursPayload": 24,
    "ExpirationHoursReports": 0.5
  },
  "BusSettings": {
    "BootstrapServers": "kafka:9092",
    "GroupId": "report-worker-group",
    "AutoOffsetReset": "Latest",
    "EnableAutoCommit": false,
    "BatchSize" : 100,
    "Topics" : {
      "ReportRequests" : "report-requests"
    }
  }
}
